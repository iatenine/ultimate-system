<div class="games">
    <div class="row justify-content-center">
        {{#each games}}
        <div class="col-sm-2" style="margin-top: 50px;">
            <div class="card">

                <img src="https://cdn.cloudflare.steamstatic.com/steam/apps/{{this.appID}}/header.jpg" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">Game Title: {{this.gametitle}}</h5>

                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>